<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.Pane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/8.0.112" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.levelup.tictactoe.controller.MainController">
   <children>
      <Pane fx:id="connectPane" prefHeight="80.0" prefWidth="600.0">
         <children>
            <Label layoutX="134.0" layoutY="3.0" prefHeight="24.0" prefWidth="323.0" text="Create a server or connect to another">
               <font>
                  <Font size="19.0" />
               </font>
            </Label>
            <Button fx:id="serverBtn" layoutX="52.0" layoutY="40.0" mnemonicParsing="false" onAction="#createServer" prefHeight="25.0" prefWidth="164.0" text="Create a Server" />
            <Button fx:id="clientBtn" layoutX="381.0" layoutY="40.0" mnemonicParsing="false" onAction="#connectToServer" prefHeight="25.0" prefWidth="152.0" text="Connect to Server" />
         </children>
      </Pane>
      <Pane fx:id="gamePane" layoutY="82.0" prefHeight="320.0" prefWidth="600.0" />
   </children>
</AnchorPane>
